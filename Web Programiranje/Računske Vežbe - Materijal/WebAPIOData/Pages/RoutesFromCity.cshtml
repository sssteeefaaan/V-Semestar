@page
@{
    Layout = "_Layout";
    ViewData["Title"] = "Routes from city";
}
@section Scripts {
    <script src="Scripts/RFC.js"></script>
}
<div class="form-row">
    <div class="col-auto mt-2 mb-2">
        <select class="form-control" name="aerodrom" id="aerodrom">
            <option value="BEG">Beograd</option>
            <option value="AGP">Malaga</option>
            <option value="ALC">Alicante</option>
            <option value="ALG">Houari</option>
            <option value="AMS">Amsterdam</option>
            <option value="ARN">Stockholm</option>
            <option value="BEN">Benina</option>
            <option value="BEY">Beirut</option>
            <option value="BHX">Birmingham</option>
            <option value="BSL">Basel</option>
        </select>
    </div>
    <div class="col-auto mt-2 mb-2">
        <input type="submit" value="Request" class="btn btn-primary" onclick="readData()">
    </div>
</div>
<table class="table table-striped table-dark">
  <thead>
    <tr>
      <th scope="col">CallSign</th>
      <th scope="col">Airport Name</th>
      <th scope="col">Airport City</th>
      <th scope="col">Airport State</th>
      <th scope="col">Airport Country</th>
      <th scope="col">Airport Elevation</th>
      <th scope="col">Airport Latitude</th>
      <th scope="col">Airport Longitude</th>
      <th scope="col">Passengers</th>
    </tr>
  </thead>
  <tbody>
  </tbody>
</table>